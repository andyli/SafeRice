{
	"manifest_version": 2,
	"name": "SafeRice",
	"version": "0.0.4",
	"description": "在OpenRice的餐廳頁面顯示相關強冠豬油商戶的搜尋結果。",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" 
    },
	"content_scripts": [
		{
			"matches" : [
				"http://www.openrice.com/*"
			],
			"js" : [
				"js/jquery.min.js",
				"js/jquery.csv.js",
				"js/fuse.min.js",
				"js/ContentScript.js"
			],
			"run_at" : "document_idle"
		}
	],
	"web_accessible_resources": [
		"js/*.map",
		"data/*"
	]
}